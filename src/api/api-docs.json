      "get" : {
        "tags" : [ "HumanTaskFileRestController" ],
        "operationId" : "getFileNames",
        "parameters" : [ {
          "name" : "taskId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
